Snake spawnNewSnake(int x, int y, String name) {
  switch (name) {
  case "EricFreeman":
    Snake snake =  new EricFreemanSnake(x, y);  // add your snake here
    snake.debug = true;
    return snake;
  case "HunterSnake":
    return new HunterSnake(x, y);
  default:
    System.out.println("No matching Snake class for name: " + name);
    return null;
  }
}

